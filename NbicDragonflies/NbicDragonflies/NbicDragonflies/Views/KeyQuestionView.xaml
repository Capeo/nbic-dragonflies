<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
			 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
			 x:Class="NbicDragonflies.Views.KeyQuestionView"
			 xmlns:utility="clr-namespace:NbicDragonflies.Utility;assembly=NbicDragonflies">
  <ContentView.Content>
    <StackLayout BackgroundColor="{x:Static utility:Constants.Background}">
      <Label x:Name="AlternativeCategory" FontSize="18" FontAttributes="Bold"/>
      <ScrollView x:Name="ScrollView" Margin="10" BackgroundColor="{x:Static utility:Constants.Background}">
        <StackLayout x:Name="StackLayout"/>
      </ScrollView>
    </StackLayout>
  </ContentView.Content>
</ContentView>
